{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "apps-on-azure-blog": {
      "appLocation": ".",
      "outputLocation": "_site",
      "appBuildCommand": "JEKYLL_ENV=production bundle exec jekyll build -b https://apps-on-azure.net",
      "run": "bundle exec jekyll serve --livereload",
      "appDevserverUrl": "http://localhost:4000",
      "appName": "apps-on-azure-site",
      "resourceGroup": "apps-on-azure"
    }
  }
}